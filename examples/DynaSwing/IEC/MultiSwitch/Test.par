<parametersSet xmlns="http://www.rte-france.com/dynawo">
  <set id="Solver">
    <par type="INT" name="order" value="2"/>
    <par type="DOUBLE" name="initStep" value="1e-5"/>
    <par type="DOUBLE" name="minStep" value="1e-5"/>
    <par type="DOUBLE" name="maxStep" value="10"/>
    <par type="DOUBLE" name="absAccuracy" value="1e-4"/>
    <par type="DOUBLE" name="relAccuracy" value="1e-4"/>
    <par type="DOUBLE" name="minimalAcceptableStep" value="1e-7"/>
  </set>
  <set id="u1">
    <par type="DOUBLE" name="setPoint_Value0" value="1.5"/>
  </set>
  <set id="u2">
    <par type="DOUBLE" name="setPoint_Value0" value="1.2"/>
  </set>
  <set id="u3">
    <par type="DOUBLE" name="setPoint_Value0" value="-0.1"/>
  </set>
</parametersSet>
